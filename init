gh re --browser false --new is-electron-builtin --description "Check if a string matches the name of a Electron builtin module." &&

rm init &&

git init &&
git remote add origin https://github.com/parro-it/is-electron-builtin.git &&
git add .  &&
git commit -m "project skeleton" &&

git push --set-upstream origin master &&

npm version patch &&
mkdir ./node_modules &&
cp -rp ../dev-dependencies ./node_modules &&
npm link &&

travis sync &&
travis enable  --no-interactive &&

git add .  &&
git commit -m "ava&xo" &&
npm version patch &&
git push --follow-tags &&

npm publish &&

echo 'Project scaffolded'
